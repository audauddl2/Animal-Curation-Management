@charset "utf-8";
/*btnWrap*/
.btnWrap{position:relative; float:left; width:100%; height:150px; line-height:150px; background-color:#F1F2F2; border-bottom:1px solid black; font-size:55px; font-family: NotoSansCJKkr-Medium;}
.columnBtn{float:left; width:32.9%; text-align:center;}
.columnText{display:inline-block; color:#5184FF;}

.borderBar{float:left; width:1px; height:55px; background-color:black; margin: 62.5px 0 0 0;}

.brandBtn{float:left; width:32.9%; text-align:center;}
.brandText{display:inline-block;}

.userBtn{float:left; width:32.9%; text-align:center;}
.userText{display:inline-block;}


/*productInfo*/
.productInfo{float:left; width:100%; text-align:center;border-bottom:1px solid black;}
.productInfoArea{display:inline-block; width:84%; margin:23px 0 23px 0; background-color:#fff;}
.iconProduct{float:left; width:160px; height:160px; margin:20px 40px 0 0; border:1px solid black;}
.productInfoText{float:left; text-align:left; margin:10px 0 0 0;}
.productName{font-size:40px; color:#939598;}
.productType{font-size:50px; margin:-12px 0 10px 0;}
.reviewGradeImg{float:left;margin:0 5px 0 0; width:40px;}
.reviewGrade{float:left; font-size:40px; margin:-12px 0 0 15px;}
.iconGrade{float:right; margin: 27px 0 0 0; width:145px;}

/*deleteBtn*/
.deleteBtn{display:inline-block; width:90%; height:150px; background-color:#5184FF; margin:100px 0 0 0; line-height:150px;}
.iconDelete{position:absolute; width:65px; margin:35px 0 0 0;}
.deleteText{display:inline-block; margin:0 0 0 75px; color:#fff; }