	}

	/**
	 * @param useIDX the useIDX to set
	 */
	public void setUseIDX(int useIDX) {
		this.useIDX = useIDX;
	}

	/**
	 * @return the useName
	 */
	public String getUseName() {
		return useName;
	}

	/**
	 * @param useName the useName to set
	 */
	public void setUseName(String useName) {
		this.useName = useName;
	}

	/**
	 * @return the writer
