	private String writeDate;					//등록일
	private String modifier;					//수정자
	private String modifyDate;					//수정일
	private String useYN;						//사용여부

	
	/**
	 * @return the dangerIDX
	 */
	public int getDangerIDX() {
		return dangerIDX;
	}

	/**
	 * @param dangerIDX the dangerIDX to set
	 */
	public void setDangerIDX(int dangerIDX) {
		this.dangerIDX = dangerIDX;
	}

	/**
	 * @return the dangerName
