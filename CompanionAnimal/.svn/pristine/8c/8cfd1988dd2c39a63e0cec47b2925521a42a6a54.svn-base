@charset "utf-8";

	.showArea{
      background-color:#fff;
      margin:180px 0 0 0;
	}
	.innerbox{
		text-align: center;
	}
	.seachpBox{
		display: inline-block;
		width: 88%;
		margin: 10% 0 2% 0;
		border-bottom: 1px solid #444444;
		height: 100px;
	}
	.seachpBox img{
		width: 5%;
		margin-right: 10px;
	}
	.searchText{
		border-bottom: none;
		height: 90px;
		font-size: 40px;
		width: 90%;
	}
	.gapBox{
		display: inline-block; 
		width: 100%;
		height: 30px;
		background-color:#F1F2F2;
		border-top: 1px solid #444444;
		border-bottom: 1px solid #444444;
	}
	.historyBox{
		height: 150px;
		border-bottom:1px solid #444444;
	}
	.historyBox p{
		color: #5184FF;
		font-size: 50px;
		display:inline-block;
		line-height:150px;
	}
	.word{
		height: 120px;
		border-bottom:1px solid #444444;
		position: relative;
	}
	.searchWord{
		position: absolute;
		left: 120px;
		top: 25px;
		float: left;
		font-size: 40px;
		color: #939598;
	}
	.deleteBox{
		height: 150px;
		border-bottom:1px solid #444444;
		text-align: center;
	}
	.delete P{
		position: absolute;
		color: #5184FF;
		font-size: 50px;
		display:inline-block;
		line-height:150px;
	}
	.delete img{
		width: 6%;
		margin: 30px;
	}