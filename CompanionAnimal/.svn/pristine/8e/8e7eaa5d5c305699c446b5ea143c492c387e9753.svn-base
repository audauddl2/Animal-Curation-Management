@charset "utf-8";
    
	.header{
		position: relative;
		float: left;
		width:100%;
		height: 200px;
		border-bottom:1px solid black;
		background-color:#ffffff;
		text-align:center;
		line-height:200px;
		font-family: 'NotoSansCJKkr-Medium';
	}
	.header p {
		display:inline-block;
		font-size: 65px;
		color: #5184FF;	
	}
	.back_btn {
		position: absolute;
		width:200px;
		height:200px;
	}
	.back_btn img{
		width: 15%;
	}

	.box{
		position: relative;
		float: left;
		width: 33.32%;
		height:auto;
		background-color: #FFF;
		border:1px solid black;
	}
	.box img{width:100%;}
	
	.outBox{
		display: inline-block; 
		width: 100%;
		margin: 52px 0 0 0;
	}

	body{
		width:100%;
		background-color:#F1F2F2;
		text-align: center;
	}
	.nav{
		position: relative; 
		float: left;
		width:100%;
	}
	.nav img{width:100%;}
	.box_nav {
		width: 25%;
		position: relative;
		float: left;
	}